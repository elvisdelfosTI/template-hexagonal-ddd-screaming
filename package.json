{
  "name": "back.hexagonal",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "doc": "tsx docs/swagger.ts",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
    "migrate:test": "prisma migrate deploy",
    "test:integration": "ENV=test docker-compose -f docker-compose.test.yml up -d && sleep 3 && npm run migrate:test && jest --config ./test/config/jest.integration.config.ts",
    "test:integration:down": "docker-compose -f docker-compose.test.yml down",
    "test:unit": "jest --config ./test/config/jest.unit.config.ts",
    "test:e2e": "jest --config ./test/config/jest.e2e.config.ts",
    "lint": "eslint  --fix",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "tsc && cpx \"src/**/*.proto\" dist/ && tsc-alias",
    "dev": "tsx --require tsconfig-paths/register --inspect --watch ./src/main.ts",
    "dev:docker": "docker-compose -f docker-compose.test.yml up -d &&  sleep 3 && npm run migrate &&tsx --inspect --watch ./src/main.ts",
    "tsc": "tsc",
    "release": "release-it --no-npm --ci --no-git.requireUpstream --verbose --no-questions"
  },
  "compilerOptions": {
    "moduleResolution": "nodenext"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.12.5",
    "@grpc/proto-loader": "^0.7.13",
    "@grpc/reflection": "^1.0.4",
    "@prisma/client": "^6.1.0",
    "@protobuf-ts/grpc-transport": "^2.9.4",
    "@protobuf-ts/plugin": "^2.9.4",
    "@protobuf-ts/runtime": "^2.9.4",
    "bcrypt": "^5.1.1",
    "bcrypt-ts": "^5.0.3",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "google-protobuf": "^3.21.4",
    "grpc-server-reflection": "^0.1.5",
    "grpc-tools": "^1.12.4",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "prisma": "^6.1.0",
    "tslib": "^2.8.1",
    "tslog": "^4.9.3",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "@jest/globals": "^29.7.0",
    "@ngneat/falso": "^7.3.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "axios": "^1.7.9",
    "babel-jest": "^29.7.0",
    "cpx": "^1.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "release-it": "^18.1.1",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.6.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.20.0",
    "yaml": "^2.7.0"
  },
  "_moduleAliases": {
    "#author": "dist/lib/Author",
    "#book": "dist/lib/Book",
    "#auth": "dist/lib/Auth",
    "#shared": "dist/lib/shared",
    "#common": "dist/common"
  }
