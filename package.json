{
  "name": "back.hexagonal",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "migrate:dev": "npx prisma migrate dev --name init",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint  --fix",
    "prepare": "husky install",
    "start": "node dist/main.js",
    "build": "npx tsc ./src/main.ts --outDir ./dist/",
    "dev": "tsx --inspect --watch ./src/main.ts",
    "tsc": "tsc",
    "version": "npx conventional-changelog -p angular -i CHANGELOG.md -s",
    "version:generate": "npx conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog"
    ]
  },
  "compilerOptions": {
    "moduleResolution": "nodenext"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@prisma/client": "^6.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.4",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^11.0.7",
    "@types/conventional-changelog-core": "^4.2.8",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "release-please": "^16.15.0",
    "semantic-release": "^21.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.20.0"
  },
  "dependencies": {
    "express": "^4.21.2",
    "import": "^0.0.6",
    "morgan": "^1.10.0",
    "tslog": "^4.9.3"
  }
}
